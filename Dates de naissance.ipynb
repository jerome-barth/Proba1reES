   "source": [
    "def prob(n):\n",
    "    accumulateur = 1.0\n",
    "    for i in range(n):\n",
    "        accumulateur *= (365-i)/365\n",
    "    return 1 - accumulateur\n",
    "\n",
    "for i in range(36):\n",
    "    print(\"Pour\", i+1, \"personnes, il y a\", round(prob(i+1)*100,1), \"% de chances qu'au moins 2 soient nées le même jour\")"
   ]
  }
